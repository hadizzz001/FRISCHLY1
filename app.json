{
  "expo": {
    "name": "myMob",
    "slug": "myMob",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mymob",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "displayName": "Frischly" 
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "com.alihadimedlej001.myMob",
      "displayName": "Frischly"  
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
    "image": "./assets/images/splash-icon.png",
    "imageWidth": 400,
    "resizeMode": "cover",
    "backgroundColor": "#ffffff"
        }
      ],
      "expo-audio",
      "expo-font",
      "expo-video",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "df83a956-257e-4bb7-8016-ac8d53265a61"
      }
    }
  }
}
